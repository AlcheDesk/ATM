        run_set_type_id,
        priority,
        copy_from_id, parent_id, uuid, aliases
    </sql>
    <select id="selectByAliases" resultMap="BaseResultMap">
        select rs.* from run_set rs
        inner join
        run_set_alias_link rsal on rsal.run_set_id = rs.id
        inner join
        alias al on al.id = rsal.alias_id
        where al.is_deleted = false and al.name in
        <foreach item="item" index="index" collection="aliases" open="(" separator="," close=")">
            #{item.name}
        </foreach>
    </select>
