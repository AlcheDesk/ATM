    <select id="selectByTestCaseIds" resultMap="BaseResultMap">
        select * from driver where driver_type_id in (
        select DISTINCT dilink.driver_type_id from
        driver_type_instruction_type_link dilink
        inner join
        instruction ins on dilink.instruction_type_id = ins.instruction_type_id
        where ins.is_deleted =
        false and ins.test_case_id in
        <foreach item="item" index="index" collection="testCaseIds" open="(" separator="," close=")">
            #{item}
        </foreach>
        )
    </select>
    <select id="selectByRunSetId" parameterType="java.lang.Long" resultMap="BaseResultMap">
        select * from driver where driver_type_id in
        (
        select DISTINCT dilink.driver_type_id from
        driver_type_instruction_type_link dilink
        inner
        join
        instruction ins on
        dilink.instruction_type_id = ins.instruction_type_id
        inner join
        run_set_test_case_link rtlink on ins.test_case_id = rtlink.test_case_id
        inner join
        run_set rs on rtlink.run_set_id = rs.id
        where ins.is_deleted =
        false and rs.id =
        #{runSetId,jdbcType=BIGINT}
        )
    </select>
